<?xml version="1.0" encoding="utf-8"?>
<sopconfig syberos:sopid="com.sinux.PhoneCall"
    syberos:versionCode="1"
    syberos:versionName="2.0.4"
    xmlns:syberos="http://schemas.syberos.com/rpm/res/syberos">
    <application syberos:debuggable="true"
        syberos:description="Brief introduction of your application here. "
        syberos:icon="res/phonecall.png"
        syberos:name="PhoneCall">
        <uiapp syberos:id="MyApp"
            syberos:exec="bin/MyApp"
            syberos:splash=""
            syberos:icon="res/phonecall.png"
            syberos:launcher = "false"
            syberos:name="电话">
            <url>
                <scheme>sinuxtel</scheme>
            </url>
        </uiapp>
        <service syberos:id="MyService"
            syberos:exec="bin/MyService">
            <receiver>
                <event>BOOT_COMPLETE</event>
            </receiver>
        </service>
    </application>
    <uses-permission syberos:name="syberos.permission.ACCESS_WAKELOCK"/>
    <uses-permission syberos:name="syberos.permission.ACCESS_CAMERA"/>
    <uses-permission syberos:name="syberos.permission.ACCESS_FLASHLIGHT"/>
    <uses-permission syberos:name="syberos.permission.RECORD"/>

</sopconfig>

