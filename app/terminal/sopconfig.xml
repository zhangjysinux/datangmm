<?xml version="1.0" encoding="utf-8"?>
<sopconfig syberos:sopid="com.sinux.terminal"
    syberos:versionCode="1"
    syberos:versionName="1.0.0"
    xmlns:syberos="http://schemas.syberos.com/rpm/res/syberos">
    <application syberos:debuggable="true"
        syberos:description="Brief introduction of your application here. "
        syberos:icon="res/TerminalApp.png"
        syberos:name="terminal">
        <uiapp syberos:id="TerminalApp"
            syberos:exec="bin/TerminalApp"
            syberos:splash=""
            syberos:icon="res/TerminalApp.png"
            syberos:launcher = "true"
            syberos:name="战术终端">
        </uiapp>
        <service syberos:id="TerminalService"
            syberos:exec="bin/TerminalService">
            <receiver>
                <event>BOOT_COMPLETE</event>
            </receiver>
        </service>
    </application>

</sopconfig>

