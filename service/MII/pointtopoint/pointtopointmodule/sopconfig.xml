<?xml version="1.0" encoding="utf-8"?>
<sopconfig syberos:sopid="com.sinux.pointtopointmodule"
    syberos:versionCode="1"
    syberos:versionName="1.0.0"
    xmlns:syberos="http://schemas.syberos.com/rpm/res/syberos">
    <application syberos:debuggable="true"
        syberos:description="PointToPointService"
        syberos:icon="res/PointToPointApp.png"
        syberos:name="pointtopointmodule">
        <uiapp syberos:id="PointToPointApp"
            syberos:exec="bin/PointToPointApp"
            syberos:splash=""
            syberos:icon="res/PointToPointApp.png"
            syberos:name="PointToPointApp">
        </uiapp>
        <service syberos:id="PointToPointService"
            syberos:exec="bin/PointToPointService">
            <receiver>
                <event>BOOT_COMPLETE</event>
            </receiver>
        </service>
    </application>
</sopconfig>

