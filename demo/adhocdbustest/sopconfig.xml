<?xml version="1.0" encoding="utf-8"?>
<sopconfig syberos:sopid="com.sinux.adhocdbustest"
    syberos:versionCode="1"
    syberos:versionName="1.0.0"
    xmlns:syberos="http://schemas.syberos.com/rpm/res/syberos">

    <uses-permission syberos:name="syberos.permission.ACCESS_APP_PERMISSION"/>
    <uses-permission syberos:name="syberos.permission.ACCESS_USERAUTH"/>
    <uses-permission syberos:name="syberos.permission.ADMIN_PACKAGES"/>

    <application syberos:debuggable="true"
        syberos:description="Brief introduction of your application here. "
        syberos:icon="res/MyApp.png"
        syberos:name="adhocdbustest">
        <uiapp syberos:id="MyApp"
            syberos:exec="bin/MyApp"
            syberos:splash=""
            syberos:icon="res/MyApp.png"
            syberos:name="MyApp">
        </uiapp>
        <service syberos:id="MyService"
            syberos:exec="bin/MyService">
            <receiver>
                <event>BOOT_COMPLETE</event>
            </receiver>
        </service>
    </application>
</sopconfig>

